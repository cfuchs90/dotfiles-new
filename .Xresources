urxvt*scrollBar: false
URxvt*.perl-ext-common: default,matcher,clipboard,url-select,keyboard-select,font-size
URxvt*keysym.M-Escape: perl:keyboard-select:active
URxvt*keysym.M-u: perl:url-select:select_next
! URxvt*keysym.M-c: eval:selection_to_clipboard
! URxvt*keysym.M-v: eval:paste_clipboard
URxvt.transparent:   true
urxvt*font: xft:dejavusansmono:size=11